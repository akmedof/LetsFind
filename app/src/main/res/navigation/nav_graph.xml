<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/baseFragment">

    <fragment
        android:id="@+id/baseFragment"
        android:name="com.epicood.letsfind.view.BaseFragment"
        android:label="Lets Find Quiz"
        tools:layout="@layout/fragment_base" >
        <action
            android:id="@+id/action_baseFragment_to_quizFragment"
            app:destination="@id/quizFragment" />
    </fragment>
    <fragment
        android:id="@+id/quizFragment"
        android:name="com.epicood.letsfind.view.QuizFragment"
        android:label="Quiz"
        tools:layout="@layout/fragment_quiz" >
        <action
            android:id="@+id/action_quizFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.epicood.letsfind.view.ResultFragment"
        android:label="Cavab"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment_to_baseFragment"
            app:destination="@id/baseFragment" />
    </fragment>
    <fragment
        android:id="@+id/ratingFragment"
        android:name="com.epicood.letsfind.view.RatingFragment"
        android:label="Reytinq"
        tools:layout="@layout/fragment_rating" />
    <fragment
        android:id="@+id/profilFragment"
        android:name="com.epicood.letsfind.view.ProfilFragment"
        android:label="Profil"
        tools:layout="@layout/fragment_profil" />
</navigation>